# Matching Service Environment Variables Template
# Copy this to .env and update values for your environment

# Server Configuration
NODE_ENV=production
PORT=4000
HOST_URL=http://localhost:3000
WS_PORT=http://localhost:5000

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DATABASE_INDEX_MATCHING_SERVICE=0

# Logging
LOG_LEVEL=info

# Health Check
HEALTH_CHECK_INTERVAL=30000