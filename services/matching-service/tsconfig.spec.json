{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "dist/spec",
    "module": "CommonJS",
    "target": "ES2020",
    "types": ["jasmine", "node"],
    "paths": {
      "redis/*": ["../../redis/*"]
    },
    "baseUrl": ".",
    "moduleResolution": "node"
  },
  "include": [
    "tests/**/*.spec.ts",
    "../../shared/**/*.ts",
    "**/*.d.ts"
  ],
  "exclude": ["node_modules", "dist"]
}
